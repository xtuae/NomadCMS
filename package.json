{
  "name": "nomad-neon",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "payload generate:importmap",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.6",
    "@headlessui/react": "^2.2.7",
    "@payloadcms/db-postgres": "^3.54.0",
    "@payloadcms/next": "^3.54.0",
    "@payloadcms/payload-cloud": "^3.54.0",
    "@payloadcms/plugin-cloud-storage": "^3.54.0",
    "@payloadcms/plugin-redirects": "^3.54.0",
    "@payloadcms/plugin-search": "^3.54.0",
    "@payloadcms/plugin-sentry": "^3.54.0",
    "@payloadcms/plugin-seo": "^3.54.0",
    "@payloadcms/richtext-lexical": "^3.54.0",
    "@payloadcms/storage-vercel-blob": "^3.54.0",
    "@payloadcms/ui": "^3.54.0",
    "@sentry/nextjs": "^10.7.0",
    "@supabase/supabase-js": "^2.55.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "express": "^5.1.0",
    "graphql": "^16.8.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "payload": "^3.54.0",
    "payload-email-template": "^1.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2",
    "sharp": "0.34.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.5.0",
    "jsdom": "26.1.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
